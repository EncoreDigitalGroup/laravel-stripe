{
    "name": "encoredigitalgroup/common-stripe",
    "type": "library",
    "authors": [
        {
            "name": "Marc Beinder",
            "email": "marc.beinder@encoredigitalgroup.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.encoredigitalgroup.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "neronmoon/scriptsdev": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "require": {
        "php": "^8.3",
        "encoredigitalgroup/stdlib": "^1.3",
        "illuminate/support": "^11.0",
        "phpgenesis/logger": "^0.2.54"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "larastan/larastan": "^2.9",
        "neronmoon/scriptsdev": "^0.1.9",
        "orchestra/testbench": "^9.1",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2",
        "phpgenesis/devutilities": "^0.2.54",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.12",
        "rector/rector": "^1.0",
        "tightenco/duster": "^3.0",
        "tomasvotruba/cognitive-complexity": "^0.2.3"
    },
    "autoload": {
        "psr-4": {
            "EncoreDigitalGroup\\Common\\Stripe\\": "src/"
        }
    },
    "scripts": {},
    "extra": {
        "laravel": {
            "providers": [
                "EncoreDigitalGroup\\Common\\Stripe\\Providers\\ServiceProvider"
            ],
            "dont-discover": []
        },
        "scripts-dev": {
            "post-update-cmd": [],
            "post-autoload-dump": []
        }
    }
}
